  "bugs": {
    "url": "https://github.com/SpacelabUK/track-api/issues"
  },
  "homepage": "https://github.com/SpacelabUK/track-api#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-mocha": "^5.2.0",
    "express": "^4.16.3",
    "mocha": "^5.0.5",
    "nyc": "^12.0.2",
    "parse-fn-args": "^1.0.1",
    "sinon": "^4.5.0",
    "supertest": "^3.0.0"
